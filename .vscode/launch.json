{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "nwjs",
            "request": "launch",
            "name": "Launch NWjs",
            "nwjsVersion": "any",
            "webRoot": "${workspaceFolder}",
            "reloadAfterAttached": true,
            "runtimeExecutable": "F:\\nwjs-sdk-v0.86.0-win-x64\\nwjs-sdk-v0.86.0-win-x64\\nw",
            "runtimeArgs": [
                ".",
                "test&noaudio&webgl"
            ],
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEVD_CONTAINER_RANDOM_ACCESS_MAX_ITEMS": "1000",
            }
        }
    ]
}